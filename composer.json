{
  "name": "s00d/rocksdb-client-php",
  "description": "A PHP client for interacting with RocksDB server",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.4 || ^8.0",
    "illuminate/support": "^7.0|^8.0|^9.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "s00d\\RocksDB\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "s00d\\RRocksDB\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "YourVendor\\RocksDB\\RocksDBServiceProvider"
      ],
      "aliases": {
        "RocksDB": "YourVendor\\RocksDB\\Facades\\RocksDB"
      }
    }
  }
}
